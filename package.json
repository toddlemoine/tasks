{
  "name": "react-kickstart",
  "version": "2.0.2",
  "description": "just another react + webpack boilerplate.",
  "private": true,
  "author": "Alessandro Arnodo <alessandro@arnodo.net> (https://alessandro.arnodo.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vesparny/react-kickstart/issues"
  },
  "homepage": "https://github.com/vesparny/react-kickstart",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require ./test/test-init --compilers js:babel/register --recursive",
    "test:watch": "NODE_ENV=test mocha --watch --reporter min --require ./test/test-init --compilers js:babel/register --recursive",
    "test:cov": "NODE_ENV=test babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --require ./test/test-init --recursive",
    "start": "node devServer",
    "build:start": "NODE_ENV=production node prodServer",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.config.prod.js --progress --profile --colors",
    "gh-pages": "./scripts/publish-gh-pages",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "dependencies": {
    "alt": "^0.17.3",
    "classnames": "^2.1.3",
    "fastclick": "^1.0.6",
    "history": "^1.9.1",
    "immutable": "^3.7.5",
    "normalize.css": "^3.0.3",
    "react": "0.14.0-rc1 ",
    "react-dom": "0.14.0-rc1",
    "react-router": "1.0.0-rc1",
    "uuid-lib": "0.0.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babel-jscs": "^2.0.4",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.0.5",
    "chai": "^3.2.0",
    "css-loader": "^0.18.0",
    "eslint": "^1.5.0",
    "eslint-plugin-react": "^3.4.2",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.6.1",
    "isparta": "^3.0.4",
    "jscs": "^2.1.1",
    "jsdom": "^6.5.1",
    "json-loader": "^0.5.3",
    "mocha": "^2.3.3",
    "mocha-jsdom": "^1.0.0",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "0.14.0-rc1",
    "react-transform-hmr": "^1.0.0",
    "rimraf": "^2.4.3",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.12.4",
    "stylus-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-hot-middleware": "^2.2.0"
  }
}
